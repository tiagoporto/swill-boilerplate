<!doctype html>
<html lang="{{#if lang}}{{lang}}{{else}}en{{/if}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{{#if title}}{{title}}{{else}}Untitled{{/if}}</title>
    <meta name="description" content="{{#if description}}{{description}}{{/if}}">
    <meta name="keywords" content="{{#if keywords}}{{keywords}}{{/if}}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="author" href="humans.txt" />
    <link rel="manifest" href="/manifest.json">

    <!-- Logos downloadtip -->
    <link rel="icon" type="image/png" href="img/logos/logo.png" data-title="Screen Bitmap file (.png)" />
    <link rel="icon" type="image/png" href="img/logos/logo-monochrome.png" data-title="Screen Monochrome Bitmap file (.png)" />
    <link rel="icon" type="image/svg+xml" href="img/logos/logo.svg" data-title="Screen Vector file (.svg)" />
    <link rel="icon" type="image/svg+xml" href="img/logos/logo-monochrome.svg" data-title="Screen Monochrome Vector file (.svg)" />
    <link rel="icon" type="image/eps" href="img/logos/logo.eps" data-title="Print file (.eps)" />
    <link rel="icon" type="image/eps" href="img/logos/logo-monochrome.eps" data-title="Print Monochrome file (.eps)" />

    <!-- Add to homescreen for Chrome on Android -->
    <meta name="theme-color" content="">
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="icon" sizes="192x192" href="img/touch/chrome-touch-icon-192x192.png">

    <!-- Add to homescreen for Safari on iOS -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="apple-mobile-web-app-title" content="{{#if title}}{{title}}{{else}}Untitle{{/if}}">
    <link rel="apple-touch-icon" href="img/touch/apple-touch-icon.png">

    <!-- build:css css/styles.combined.min.css -->
    <link rel="stylesheet" href="../node_modules/normalize.css/normalize.css">
    <link rel="stylesheet" href="../node_modules/outdatedbrowser/outdatedbrowser/outdatedbrowser.css">
    <link rel="stylesheet" href="../node_modules/jquery-logo-downloadtip/css/jquery-logo-downloadtip.css">
    <link rel="stylesheet" href="css/styles.css">
    <!-- endbuild -->
  </head>
  <body>
    <!-- ============= Outdated Browser ============= -->
    <div id="outdated"></div>
    <!-- ============================================ -->

    <header class="header">
      <h1>
          <a href="index.html">
              <img id="logo" src="img/logos/logo.svg" alt="Swill Boileplate Logo" width="250">
          </a>
      </h1>

      <p>The front-end Boilerplate that you need to start easily customizable multi-device development.</p>

      <a href="https://github.com/tiagoporto/generator-swill-boilerplate" class="github-corner">
